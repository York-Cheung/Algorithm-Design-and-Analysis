{
    "code-runner.executorMap": {
        "cpp": "cd $dir && rm ./$fileNameWithoutExt && g++ -std=c++11 $fileName -o $fileNameWithoutExt && ./$fileNameWithoutExt"
    },
    "code-runner.runInTerminal": true,
    "code-runner.saveAllFilesBeforeRun": true,
    "python.linting.pylintEnabled": false,
    "files.associations": {
        "set": "cpp",
        "string": "cpp",
        "vector": "cpp",
        "__bit_reference": "cpp",
        "__split_buffer": "cpp",
        "__tree": "cpp",
        "bitset": "cpp",
        "initializer_list": "cpp",
        "iterator": "cpp",
        "random": "cpp",
        "algorithm": "cpp"
    },
    "fileheader.Author": "Yorkson",
    "fileheader.LastModifiedBy": "Yorkson"
} 